{
  "author": "LiveIntent",
  "repository": {
    "type": "git",
    "url": "https://github.com/liveintent-berlin/live-connect"
  },
  "description": "LiveConnect, The first party identity provider",
  "version": "2.2.0",
  "versionPrefix": "live-connect-v",
  "license": "Apache-2.0",
  "private": false,
  "name": "live-connect-js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test:unit": "npm run lint && nyc mocha --config test-config/mocha.unit.json --require @babel/register ",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:it:browserstack": "npm run build && wdio test-config/wdio.browserstack.conf.js",
    "test:it:docker:chrome": "npm run build && wdio test-config/wdio.docker.chrome.conf.js",
    "build": "npm run test:unit && rollup -c rollup-iife.config.js && rollup -c rollup-modules.config.js",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "fix-js": "eslint 'src/**/*.js' 'test/**/*.js' --fix || exit 0",
    "release:ci:patch": "release-it patch --ci",
    "release:ci:minor": "release-it minor --ci",
    "release:ci:major": "release-it major --ci"
  },
  "dependencies": {
    "tiny-hashes": "1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "@kiosked/ulid": "^3.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-strip": "^1.3.3",
    "@wdio/browserstack-service": "5.13.2",
    "@wdio/cli": "5.15.0",
    "@wdio/junit-reporter": "5.15.0",
    "@wdio/local-runner": "5.15.0",
    "@wdio/mocha-framework": "5.15.0",
    "@wdio/spec-reporter": "5.14.5",
    "@wdio/sync": "5.14.4",
    "abab": "^2.0.5",
    "acorn": "^7.2.0",
    "autoprefixer": "^9.8.0",
    "babel-core": "^6.26.3",
    "browser-cookie": "^1.2.1",
    "browser-cookies": "^1.2.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "codecov": "^3.7.2",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dirty-chai": "^2.0.1",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-wdio": "^5.13.2",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "mocha": "^6.2.3",
    "mocha-jsdom": "^2.0.0",
    "mocha-junit-reporter": "^2.0.0",
    "mock-browser": "^0.92.14",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "release-it": "^14.0.2",
    "rollup": "^2.18.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "serialize-javascript": ">=3.1.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "tiny-uuid4": "^1.0.1",
    "wdio-docker-service": "2.1.2"
  }
