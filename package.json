{
  "author": "LiveIntent",
  "repository": {
    "type": "git",
    "url": "https://github.com/liveintent-berlin/live-connect"
  },
  "description": "LiveConnect, The first party identity provider",
  "version": "1.0.9",
  "versionPrefix": "live-connect-v",
  "license": "Apache-2.0",
  "private": false,
  "name": "live-connect-js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test:unit": "npm run lint && nyc mocha --config test-config/mocha.unit.json --require @babel/register ",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:it:browserstack": "npm run lint && npm run build && wdio test-config/wdio.browserstack.conf.js",
    "test:it:docker:chrome": "npm run lint && npm run build && wdio test-config/wdio.docker.chrome.conf.js",
    "build": "npm run test:unit && rollup -c rollup-iife.config.js && rollup -c rollup-modules.config.js",
    "lint": "eslint \"src/**/*.js\"",
    "fix-js": "eslint \"src/**/*.js\" --fix || exit 0",
    "release:ci:patch": "release-it patch --ci",
    "release:ci:minor": "release-it minor --ci",
    "release:ci:major": "release-it major --ci"
  },
  "dependencies": {
    "@kiosked/ulid": "^3.0.0",
    "abab": "^2.0.2",
    "browser-cookies": "^1.2.0",
    "tiny-hashes": "1.0.1",
    "tiny-uuid4": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@rollup/plugin-replace": "^2.2.1",
    "@rollup/plugin-strip": "^1.3.1",
    "@wdio/browserstack-service": "5.13.2",
    "@wdio/cli": "5.15.0",
    "@wdio/junit-reporter": "5.15.0",
    "@wdio/local-runner": "5.15.0",
    "@wdio/mocha-framework": "5.15.0",
    "@wdio/spec-reporter": "5.14.5",
    "@wdio/sync": "5.14.4",
    "acorn": "^7.1.0",
    "autoprefixer": "^9.6.4",
    "babel-core": "^6.26.3",
    "browser-cookie": "^1.2.1",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "codecov": "^3.6.1",
    "core-js": "^3.2.1",
    "cors": "^2.8.5",
    "critical": "^1.3.6",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "mocha": "^6.2.2",
    "mocha-jsdom": "^2.0.0",
    "mocha-junit-reporter": "^1.23.1",
    "mock-browser": "^0.92.14",
    "mock-require": "^3.0.3",
    "nyc": "^14.1.1",
    "release-it": "^12.4.3",
    "rollup": "^1.27.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.4.0",
    "wdio-docker-service": "2.1.2"
  }
}
